   "source": [
    "\n",
    "class Solution(object):\n",
    "    def separateDigits(self, nums):\n",
    "        t = []\n",
    "        \n",
    "        nums = [[t.append(j) for j in str(i)] for i in nums]\n",
    "        \n",
    "        return [int(i) for i in t]\n",
    "        \n",
    "        \n",
    "s = Solution()\n",
    "nums = [7,1,3,9]\n",
    "print(s.separateDigits(nums))\n",
    "\n",
    "\n"
   ]
